                <tr>
                    <%= render :partial => 'sidebar' %>
                    <%= render :partial => 'earth' %>
                    <%= render :partial => 'map' %>
                </tr>
            </table>
        </td>
    </tr>
</table>
<%= render :partial => 'charts'%>