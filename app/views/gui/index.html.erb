<% content_for :head do%>
    <%= render :partial => 'head' %>
    <%= javascript_include_tag :all, :cache => true %>
    <%= stylesheet_link_tag 'gui' %>
    <%= stylesheet_link_tag 'simple-slider' %>
<% end %>
